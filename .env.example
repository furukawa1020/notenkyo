# Weather API Configuration
# OpenWeatherMap API Key (https://openweathermap.org/api)
# 無料プランで十分な機能が利用可能
NEXT_PUBLIC_OPENWEATHER_API_KEY=your_api_key_here

# Weather API 設定
NEXT_PUBLIC_WEATHER_CACHE_TIMEOUT=1800000
NEXT_PUBLIC_WEATHER_FALLBACK_ENABLED=true

# 位置情報設定
NEXT_PUBLIC_DEFAULT_LATITUDE=35.6762
NEXT_PUBLIC_DEFAULT_LONGITUDE=139.6503
NEXT_PUBLIC_DEFAULT_CITY=Tokyo

# デバッグモード
NEXT_PUBLIC_DEBUG_WEATHER=false
